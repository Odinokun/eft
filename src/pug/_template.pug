block variables

doctype html
html(lang="ru")
  head
    meta(charset="utf-8")
    title #{title}

    meta(name="description" content="EFT group")
    meta(name="keywords"    content="EFT group")
    meta(name="author"      content="odinokun")

    // begin responsive
    meta(name="viewport"              content="width=device-width, initial-scale=1")
    meta(http-equiv="x-ua-compatible" content="ie=edge")
    // end responsive

    // begin social content&image
    meta(property='og:title'  content='EFT group')
    meta(property='og:url'    content='http://odinokun.com/')
    meta(property="og:image"  content="assets/img/social.jpg")
    // end social content&image

    // begin favicon block start
    link(rel='shortcut icon'    type='image/x-icon' href='assets/img/favicon/favicon.png')
    // end favicon block end

    style.
      .let,.st0{fill:transparent}.index-top-section{position:relative;overflow:hidden}.preloader{position:absolute;z-index:100;top:0;right:0;bottom:0;left:0;display:flex;align-items:center;justify-content:center;width:100%;height:640px;background-color:#43ac34}@media (max-width:550px){.preloader{height:550px}}.preloader-icon{width:90%;max-width:380px}.preloader-icon svg{width:100%;height:70px;fill:transparent;stroke:#fff;stroke-width:2px}.st0{cursor:pointer;transition:.5s;animation:logoDash 1.5s linear forwards;animation-delay:1s;stroke-linecap:square;stroke-dasharray:997;stroke-dashoffset:997}.let{animation:logoOpacity .5s 1.5s linear forwards;opacity:0}.let-01{animation:logoOpacity .5s 2.1s linear forwards}.let-02{animation:logoOpacity .5s 2s linear forwards}.let-03{animation:logoOpacity .5s 1.9s linear forwards}.let-04{animation:logoOpacity .5s 1.8s linear forwards}.let-05{animation:logoOpacity .5s 1.7s linear forwards}.let-06{animation:logoOpacity .5s 1.6s linear forwards}.let-07{animation:logoOpacity .5s 1.5s linear forwards}.let-08{animation:logoOpacity .5s 1.4s linear forwards}@keyframes logoDash{from{stroke-dashoffset:997}to{stroke-dashoffset:0}}@keyframes logoOpacity{from{opacity:0}to{opacity:1}}

    link(rel='stylesheet' href='assets/css/foundation.css')
    link(rel='stylesheet' href='assets/css/app.css')
    link(rel='stylesheet' href='assets/css/uncompressed.css')

    // begin custom browsers color
    meta(name='theme-color', content='#43ac34')
    // end custom browsers color

    //[if lt IE 9]><script src="http://cdnjs.cloudflare.com/ajax/libs/html5shiv/3.7.2/html5shiv.min.js"></script><![endif]

  body
    block content



