//begin parallax
.parallax-wrap
  .parallax-decor.parallax-decor--left
    svg
      use(xlink:href="assets/img/sprite.svg#decor")
  .parallax-decor.parallax-decor--right
    svg
      use(xlink:href="assets/img/sprite.svg#decor")

  .parallax#parallax
    .parallax__cloud.parallax__cloud-01
    .parallax__cloud.parallax__cloud-02
    //.parallax__cloud.parallax__cloud-03
    .parallax__cloud.parallax__cloud-04
    .parallax__cloud.parallax__cloud-05
    .parallax__cloud.parallax__cloud-06
    //.parallax__cloud.parallax__cloud-07
    .parallax__cloud.parallax__cloud-08
    .parallax__cloud.parallax__cloud-09
    .parallax__cloud.parallax__cloud-10
    //.parallax__cloud.parallax__cloud-11
    .parallax__cloud.parallax__cloud-12
    .parallax__cloud.parallax__cloud-13
    .parallax__cloud.parallax__cloud-14
    //.parallax__cloud.parallax__cloud-15
    .parallax__cloud.parallax__cloud-16
    .parallax__bg.parallax__bg--landscape(data-depth="0.15")
      .parallax__bg-inn
    .parallax__bg.parallax__bg--title(data-depth="0.05")
      .parallax__bg-inn
        h2.parallax__title EFT
    .parallax__bg.parallax__bg--main
      .parallax__bg-inn
    .parallax__bg.parallax__bg--leaf(data-depth="-0.3")
      .parallax__bg-inn
  //.parallax#parallax
    .parallax__bg.parallax__bg--03(data-depth="0.1")
      .parallax__bg-inn
    .parallax__bg.parallax__bg--text
      h2.parallax__title EFT
    .parallax__bg.parallax__bg--02
      .parallax__bg-inn
    .parallax__bg.parallax__bg--01(data-depth="-0.3")
      .parallax__bg-inn
  .parallax-footer
    .parallax-footer__text
      span
        | Простые решения
        br
        | для сада под ключ
    a(href="#about-sec").parallax-footer__btn
      svg
        use(xlink:href="assets/img/sprite.svg#arrow")
